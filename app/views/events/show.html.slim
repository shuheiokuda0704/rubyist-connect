.event-header
  h1.event-title= @event.name
  p.event-edit-link= link_to fa_icon('edit', text: '編集'), edit_event_path(@event)
  = "#{@event.participations.count} 人"
- if @event.url.present?
  p.event_url = link_to @event.url, @event.url
= render partial: 'users/user', collection: @event.users, as: :user

